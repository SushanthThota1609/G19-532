// LangGraph SQL Viz Flow
digraph {
	rankdir=LR
	supervisor [label="Supervisor Node"]
	sql_agent [label="SQL Agent"]
	viz_agent [label="Visualization Agent"]
	final_summary [label="Final Summary"]
	END [label=End shape=doublecircle]
	supervisor -> sql_agent [label="if no sql_query"]
	supervisor -> viz_agent [label="if df exists"]
	supervisor -> final_summary [label=else]
	sql_agent -> supervisor
	viz_agent -> supervisor
	final_summary -> END
}
